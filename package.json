{
  "name": "beachvollleyball-scoreboard",
  "description": "Scoreboard for the Lazy Volleyball Referee",
  "version": "0.0.1",
  "private": true,
  "main": "app.js",
  "engines": {
    "node": "8"
  },
  "xo": {
    "global": [
      "describe",
      "it"
    ],
    "ignores": [
      "test/**"
    ],
    "extends": [
      "xo/esnext",
      "xo-react"
    ],
    "rules": {
      "jsx-sort-props": 0
    }
  },
  "dependencies": {
    "@material-ui/core": "^4.12.4",
    "@material-ui/icons": "^4.11.3",
    "@material-ui/styles": "^4.11.5",
    "async-reactor": "1.2.1",
    "babel-preset-stage-0": "6.24.1",
    "babelify": "7.3.0",
    "bluebird": "3.5.0",
    "body-parser": "1.17.2",
    "bootstrap": "3.3.7",
    "bulma": "0.5.1",
    "chrome-remote-interface": "0.24.4",
    "const": "1.0.0",
    "debug": "3.0.0",
    "express": "4.15.4",
    "firebase": "4.3.0",
    "immutable": "3.8.1",
    "isomorphic-parse": "0.0.3",
    "moment": "2.29.4",
    "next": "^12.2.4",
    "preact": "8.2.1",
    "preact-compat": "3.17.0",
    "printf": "0.2.5",
    "react": "^18.2.0",
    "react-bootstrap": "0.31.2",
    "react-color": "2.13.4",
    "react-dom": "^18.2.0",
    "react-overdrive": "0.0.8",
    "react-qr-code": "^2.0.7",
    "react-textfit": "^1.1.0",
    "react-toggle": "4.0.1",
    "react-transition-group": "^4.4.1",
    "sendgrid": "5.2.2",
    "slugify2": "0.0.2",
    "styled-components": "^5.3.5",
    "superagent": "3.5.2",
    "tide": "2.0.0"
  },
  "scripts": {
    "start": "DEBUG='*,-babel' node app.js",
    "url": "0.11.0",
    "vinyl-source-stream": "1.1.0",
    "dev": "node app.js",
    "build": "next build",
    "test": "jest",
    "jest:watch": "npm test -- --watch",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "heroku-postbuild": "next build"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/nvbf/beachvollleyball-scoreboard"
  },
  "devDependencies": {
    "@storybook/react": "3.2.5",
    "eslint-config-xo-react": "0.13.0",
    "eslint-plugin-react": "7.2.1",
    "jest": "20.0.4",
    "nightwatch": "0.9.16",
    "prettier-check": "1.0.0",
    "storybook": "0.0.1",
    "xo": "0.18.2"
  }
}
